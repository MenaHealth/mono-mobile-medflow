{
  "name": "mono-mobile-medflow",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "NEXT_DISABLE_ESLINT=1 turbo run build",
    "buildWeb": "NEXT_DISABLE_ESLINT=1 turbo run build --filter=web",
    "buildMobile": "NEXT_DISABLE_ESLINT=1 turbo run build --filter=mobile",

    "dev": "turbo run dev",
    "devWeb": "turbo run dev --filter=web",
    "devMobile": "turbo run dev --filter=mobile",

    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.4",
    "prettier": "^3.4.2",
    "turbo": "^2.4.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.4",
  "dependencies": {
    "@types/next-auth": "3.5.1",
    "next": "^14.2.17",
    "next-auth": "4.24.11",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "crypto-browserify": "^3.12.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "assert": "^2.0.0",
    "stream-http": "^3.2.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "url": "^0.11.3"
  }
}
