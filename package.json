{
  "name": "mono-mobile-medflow",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "buildWeb": "turbo run build --filter=web",
    "buildMobile": "turbo run build --filter=wmobile",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.4",
    "prettier": "^3.4.2",
    "turbo": "^2.4.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.4",
  "dependencies": {
    "@types/next-auth": "3.5.1",
    "next": "^14.2.17",
    "next-auth": "4.24.11",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0"
  }
}
